<section class="modal" id="J__settings">
  <div class="header"><button class="close" data-toggle="dismiss">&times;</button><div>设置</div></div>
  <div class="body">
    <table class="nowrap simple conf">
      <tr class="o" o="main_lines">
        <td>主指标</td>
        <td>
          <ul>
            <li class="v" v="mas" default="yes">MA</li>
            <li class="v" v="emas">EMA</li>
            <li class="v" v="bbands">BBANDS</li>
            <li class="v" v="sar">SAR</li>
            <li class="v" v="none">None</li>
          </ul>
        </td>
      </tr>
      <tr class="o" o="stick_style">
        <td>主图类型</td>
        <td>
          <ul>
            <li class="v" v="candle_stick">CandleStick</li>
            <li class="v" v="candle_stick_hlc">CandleStickHLC</li>
            <li class="v" v="ohlc">OHLC</li>
            <li class="v" v="line">Line</li>
            <li class="v" v="line_o">Line-o</li>
            <li class="v" v="none">None</li>
          </ul>
        </td>
      </tr>
      <tr class="o" o="line_style" parents="stick_style:line_o|stick_style:line">
        <td>Line Style</td>
        <td>
          <ul>
            <li class="v" v="c">Close</li>
            <li class="v" v="m">Median Price</li>
            <li class="v" v="w">Volume Weighted</li>
          </ul>
        </td>
      </tr>
      <tr class="o" o="scale">
        <td>坐标比例</td>
        <td>
          <ul>
            <li class="v" v="normal">线性</li>
            <li class="v" v="logarithmic">对数</li>
          </ul>
        </td>
      </tr>
      <tr class="o hide" o="depth_range">
        <td>Depth Range<abbr title="Show the depth hints until bid or ask is less or more than average order price n%">?</abbr></td>
        <td>
          <ul>
            <li class="v" v="p1">1%</li>
            <li class="v" v="p2" default="yes">2%</li>
            <li class="v" v="p5">5%</li>
            <li class="v" v="pmax">Max</li>
            <li class="v" v="none">None</li>
          </ul>
        </td>
      </tr>
      <tr class="o hide" o="volume_currency">
        <td>Volume Currency</td>
        <td>
          <ul>
            <li class="v" v="base">Base</li>
            <li class="v" v="quote">Quote</li>
          </ul>
        </td>
      </tr>
      <tr class="o hide" o="time_zone">
        <td>时区</td>
        <td>
          <ul>
            <li class="v" v="local">本地</li>
            <li class="v" v="utc">UTC</li>
          </ul>
        </td>
      </tr>
      <tr class="o hide" o="chart_range">
        <td>Chart Range<abbr title="Limit chart&#039;s price range. It&#039;s helpful when extream long candlestick makes chart unreadable.">?</abbr></td>
        <td>
          <ul>
            <li class="v" v="auto" default="yes">Auto</li>
            <li class="v" v="pmax">Max</li>
          </ul>
        </td>
      </tr>
      <tr class="o" o="theme">
        <td>主题</td>
        <td>
